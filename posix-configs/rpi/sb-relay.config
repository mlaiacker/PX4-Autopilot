# config for 
uorb start
param load
#param set MAV_BROADCAST 1
sleep 1

if param compare SYS_AUTOSTART 0
then
	param set MAV_TYPE 5
	
	param set BAT_N_CELLS 	4
	param set BAT_A_PER_V	1.0
	param set BAT_CAPACITY		10000.0
	param set BAT_CNT_V_CURR		8.0566405E-4
	param set BAT_CNT_V_VOLT		8.0566405E-4
	param set BAT_CRIT_THR		0.07
	param set BAT_EMERGEN_THR		0.05
	param set BAT_LOW_THR		0.15
	param set BAT_R_INTERNAL	1
	param set BAT_SOURCE		1
	param set BAT_V_CHARGED		4.10
	param set BAT_V_DIV		15.468
	param set BAT_V_EMPTY		3.30
	param set BAT_V_LOAD_DROP		0.0
	param set BAT_V_OFFS_CURR		0.0
	param set SYS_AUTOSTART 4001
fi

dataman start

gps start -d /dev/serial/by-id/usb-u-blox_AG_-_www.u-blox.com_u-blox_GNSS_receiver-if00 -i uart -s

sensors start
ms5525_airspeed  start

batt_pac17xx start -b 1 -r 0.5 -s 10

temp_lm75 start -i 0 -b 1 -n tempRelay

#commander start
mavlink start -x -u 14556 -t 127.0.0.1 -o 14550 -r 1000000

sleep 1

logger start -e -t

mavlink boot_complete
